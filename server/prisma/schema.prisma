generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model auth {
  no      Int     @id @default(autoincrement())
  id      String  @unique
  pw      String
  name    String?
  phone   String?
  address String?
  email   String  @unique(map: "User_email_key")
}
